<link rel="stylesheet" href="/css/navbar.css">

<div class="navbar">
    <div class="logoContainer">
        <a href="/"><img src="data:image/png;base64," alt=""></a>
    </div>

    <div class="searchContainer">
        <form action="#" method="post">
            <input type="text" placeholder="Search for Products, Brand and More">
        </form>
    </div>

    <div class="menuContainer">
        <span class="cnt1">
            <% if (!isAuthenticated) { %>
                <a href="/user/log-in">
                    <ion-icon name="person-outline" class="icons"></ion-icon>
                    <span>Log In</span>
                </a>
            <% } else { %>
                <a href="/user/log-out">
                    <ion-icon name="person-outline" class="icons"></ion-icon>
                    <span>Log Out</span>
                </a>
            <% } %>
        </span>
        <span class="cnt1">
            <a href="/cart/get-cart-items">
                <ion-icon name="cart-outline" class="icons"></ion-icon>
                <span>Cart</span>
            </a>
        </span>
        <% if(isAdmin){ %>
            <span class="cnt1">
                <a href="/admin/add-product">
                    <ion-icon name="help-circle-outline" class="icons"></ion-icon>
                    <span>Admin</span>
                </a>
            </span>
        <% } %>
    </div>
    
</div>